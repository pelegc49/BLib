<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="411.0" prefWidth="427.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.client.ExtendTimeController">
	<children>
 	  <Button id="btnBack" fx:id="btnBack" layoutX="371.0" layoutY="372.0" mnemonicParsing="false" onAction="#backBtn" text="Back" />
      <Button id="btnExtend" fx:id="btnExtend" layoutX="14.0" layoutY="372.0" mnemonicParsing="false" onAction="#extendBtn" text="Extend" />
      <TableView id="tableBook" fx:id="tableBook" layoutX="13.0" layoutY="12.0" prefHeight="352.0" prefWidth="401.0">
        <columns>
          <TableColumn id="columnCheckBox" fx:id="columnCheckBox" minWidth="0.0" prefWidth="22.0" text="Select All">
             <graphic>
                <CheckBox id="checkBoxSelectAll" fx:id="checkBoxSelectAll" mnemonicParsing="false" onMouseClicked="#selectAllBtn" />
             </graphic>
          </TableColumn>
          <TableColumn id="columnAuthor" fx:id="columnAuthor" prefWidth="60.0" text="author" />
          <TableColumn id="columnTitle" fx:id="columnTitle" prefWidth="124.0" text="title" />
          <TableColumn id="columnDueDate" fx:id="columnDueDate" prefWidth="94.0" text="due date" />
          <TableColumn id="columnErrorMessage" fx:id="columnErrorMessage" prefWidth="100.0" text="message" />
        </columns>
      </TableView>
      <Label fx:id="lblError" layoutX="79.0" layoutY="376.0" prefHeight="17.0" prefWidth="174.0" textFill="#009600">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
	</children>
</AnchorPane>
